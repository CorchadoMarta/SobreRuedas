<div ng-app="testUser" ng-controller="hacerTest" >

    <div class="container">
        <div id="panel-form" ng-init='loadEvents()'>
            <div>
                <h2  class='col-sm-4'>HOLA!</h2>
                <label>Notificaciones:</label>
                <p> Ahora lo miro </p>
                <p>{{"fallos: " + ultimosTest[0].fallos + " fecha:" + ultimosTest[0].fecha}}</p>
                <button class='btn btn-default'>Boton
                </button>
            </div>


            <div class="modal fade" id="ISesion" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" data-keyboard="false">
