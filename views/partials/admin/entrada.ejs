                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class='modal-title align-center' id='myModalLabel'>Test</h4>
                        </div>
                        <div class="modal-body">
                            <br>
                        </div>
                        <div class="modal-footer align-center">


                            <div class='form-group'>
                             <div class='col-sm-offset-2 col-sm-10'>
                                 <button ng-hide="acabado" class='btn btn-default' data-dismiss='modal'>Cancelar
                                 </button>
                             </div>
                             <div class='col-sm-offset-2 col-sm-10'>
                                 <button ng-hide="acabado" type='submit' class='btn btn-default' ng-click='corregir()'>Acabar
                                 </button>
                             </div>
                             <div  ng-show="acabado" class='col-sm-offset-2 col-sm-10'>
                                 <button type='submit' class='btn btn-default' data-dismiss='modal' ng-click='reset()'>Finalizar
                                 </button>
                                 <h3>{{ fallos }} fallos</h3>
                             </div>
                         </div>

                     </div>
                 </div>
             </div>
         </div>
     </div>
 </div>
