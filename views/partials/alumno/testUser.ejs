                                <test ng-hide="position == 30" ng-show="position != 30"></test>
                                <fin ng-hide="position != 30" ng-show="position == 30"></fin>
                            </div>
                            <div class="modal-footer align-center">


                                    <ul class="pagination pagination-sm">
                                       <li >
                                          <a aria-label="Previous" ng-click="move(-1)" >
                                            <span aria-hidden="true">&laquo;</span>
                                        </a>
                                    </li>
                                    <li ng-repeat="x in eventSource" value="{{ $index }}" ng-model="disa" name="index" ng-click="moveIndex($index)"><a href="#">{{ $index }}</a> 
